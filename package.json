{
  "name": "OperationSilkScarf",
  "version": "2.0.0",
  "scripts": {
    "develop": "concurrently \"tsc -w -p ./server\" \"tsc -w -p ./client\" \"nodemon ./server/bin/www\" ",
    "forever": "tsc -p ./server && forever start ./server/bin/www --watch",
    "twc:client:w": "tsc -w -p ./client",
    "twc:server:w": "tsc -w -p ./server",
    "bundle:prod": "node tools/builder.js --prod",
    "postinstall": "typings install && tsc -p ./server",
    "start": "tsc && concurrently \"npm run tsc:w\" \"npm run lite\" ",
    "lite": "lite-server",
    "postinstall": "typings install && tsc -p ./server",
    "tsc": "tsc",
    "tsc:w": "tsc -w",
    "typings": "typings",
    "clean": "rm -rf dist",
    "serve": "http-server . -p 5556",
    "build": "npm run clean && tsc",
    "build_prod": "npm run build && browserify -s main app/main.js > app/bundle.js && npm run minify",
    "minify": "uglifyjs app/bundle.js --screw-ie8 --compress --mangle --output app/bundle.min.js"
  },
  "license": "MIT",
  "author": "Peter David Carter",
  "dependencies": {
    "@angular/common": "~2.1.0",
    "@angular/compiler": "~2.1.0",
    "@angular/core": "~2.1.0",
    "@angular/forms": "~2.1.0",
    "@angular/http": "~2.1.0",
    "@angular/platform-browser": "~2.1.0",
    "@angular/platform-browser-dynamic": "~2.1.0",
    "@angular/router": "~3.1.0",
    "@angular/upgrade": "~2.1.0",
    "angular-in-memory-web-api": "~0.1.5",
    "angular2-jwt": "^0.1.24",
    "@types/body-parser": "0.0.32",
    "@types/express": "^4.0.32",
    "@types/jsonwebtoken": "^7.1.31",
    "@types/serve-favicon": "^2.2.27",
    "body-parser": "~1.13.2",
    "cookie-parser": "~1.3.5",
    "debug": "^2.2.0",
    "express": "^4.13.4",
    "express-jwt": "^3.3.0",
    "forever": "^0.15.2",
    "forever-monitor": "^1.7.0",
    "jsonwebtoken": "^5.7.0",
    "serve-favicon": "~2.3.0",
    "base64-js": "^1.2.0",
    "bootstrap": "^3.3.7",
    "buffer": "^5.0.0",
    "convert-hex": "^0.1.0",
    "convert-string": "^0.1.0",
    "core-js": "^2.4.1",
    "ieee754": "^1.1.8",
    "isarray": "^2.0.1",
    "js-base64": "^2.1.9",
    "preboot": "^4.5.2",
    "reflect-metadata": "^0.1.8",
    "rxjs": "5.0.0-beta.12",
    "sha256": "^0.2.0",
    "socket.io": "^1.5.0",
    "systemjs": "0.19.39",
    "zone.js": "^0.6.25"
  },
  "devDependencies": {
    "concurrently": "^3.0.0",
    "jasmine-core": "^2.5.2",
    "karma": "^1.3.0",
    "karma-chrome-launcher": "^2.0.0",
    "karma-firefox-launcher": "^1.0.0",
    "karma-htmlfile-reporter": "^0.3.4",
    "karma-jasmine": "^1.0.2",
    "lite-server": "^2.2.2",
    "typescript": "^2.0.3",
    "typings": "^1.4.0",
    "uglify": "^0.1.5",
    "ng-semantic": "^1.1.11",
    "nodemon": "^1.9.1",
    "systemjs-builder": "^0.15.26",
    "yargs": "^4.8.1"
  }
}
